/*
 * The mapping to ISO 20022 is based upon the FIX MiFID II specification and assumes that the values specified by ESMA
 * will be incorporated as part of the ISO 20022 standard 
 */
enum BrokerCapacityEnum <"The enumeration values to specify the capacity in which the broker is involved in the transaction, when applicable. This definition has been updated to comply with MiFID II.">
	[synonym FIX value LastCapacity tag 29]
{
	Agent <"Corresponds to an execution on a public market or from an external party (e.g. broker) conducted on an agency basis.">
		[synonym FIX value "1" definition "1 = Agent"]
		[synonym ISO_20022 value "AOTC"],
	CrossAsAgent <"Corresponds to an execution between two clients of the executing firm, taking place within the firm, and conducted on an agency basis.">
		[synonym FIX value "2" definition "2 = Cross as agent"]
		[synonym ISO_20022 value "AOTC"],
	CrossAsPrincipal <"Corresponds to execution on a public market or from an external party (e.g. broker) conducted on a matched principal basis in line with the MiFID definition of ‘MTCH’ capacity.">
		[synonym FIX value "3" definition "3 = Cross as principal"]
		[synonym ISO_20022 value "MTCH"],
	Principal <"Corresponds to an execution where the executing firm is committing capital (e.g. a risk fill or change in price on fills obtained from markets or external parties).">
		[synonym FIX value "4" definition "4 = Principal"]
		[synonym ISO_20022 value "DEAL"],
	RisklessPrincipal <"Corresponds to an execution on a public market or from an external party (e.g. broker) conducted on a riskless principal basis.">
		[synonym FIX value "5" definition "5 = Riskless Principal"]
		[synonym ISO_20022 value "DEAL"]
}

